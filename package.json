{
  "name": "lixiang-auto-salon",
  "version": "1.0.0",
  "description": "Веб-приложение для салона по продаже LiXiang автомобилей",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "start:backend": "node backend/server.js",
    "start:frontend": "http-server frontend -p 8080 -c-1",
    "dev": "nodemon backend/server.js",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "live-server frontend --port=5173 --no-browser",
    "dev:all": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "setup-db": "node database/setup.js",
    "test": "jest"
  },
  "keywords": ["автосалон", "LiXiang", "веб-приложение", "база-данных"],
  "author": "Georgy Ardbelava",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "http-server": "^14.1.1"
  }
}
